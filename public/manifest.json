{
    "manifest_version": 3,
    "name": "Academic Weapon",
    "version": "1.0",
    "description": "Facilitate Top-Down Learning",
    "minimum_chrome_version": "116",
    "background": {
      "service_worker": "service-worker.js",
      "type": "module"
    },
    "side_panel": {
      "default_path": "index.html"
    },
    "icons": {
      "16": "icon32.png",
      "32": "icon32.png",
      "48": "icon32.png",
      "128": "icon32.png"
    },
    "permissions": ["sidePanel", "contextMenus", "scripting", "storage", "activeTab"],
    "action": {  },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
  }
  